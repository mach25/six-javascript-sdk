{
  "name": "six-sdk",
  "version": "0.1.1",
  "description": "SIX Javascript SDK",
  "main": "lib",
  "scripts": {
    "start" : "webpack-dev-server",
    "prepublish": "npm run compile",
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "node node_modules/babel-cli/bin/babel.js src --out-dir lib --presets es2015 --plugins transform-object-rest-spread,transform-class-properties",
    "dist": "npm run dist:dev && npm run dist:min",
    "dist:dev": "webpack",
    "dist:min": "webpack -p --output-filename six-sdk.min.js",
    "lint": "eslint src",
    "clean": "rm -fr dist lib"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/six-sdk/six-javascript-sdk.git"
  },
  "author": "Andreas Bielk <andreas.bielk@six-group.se> (http://www.six.se)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/six-sdk/six-javascript-sdk/issues"
  },
  "homepage": "https://github.com/six-sdk/six-javascript-sdk#readme",
  "keywords": [
    "six",
    "sdk"
  ],
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-class-properties": "^6.4.0",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.5.0",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-react": "^3.14.0",
    "eslint-plugin-standard": "^1.3.1",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-runtime": "^6.3.19"
  }
}
